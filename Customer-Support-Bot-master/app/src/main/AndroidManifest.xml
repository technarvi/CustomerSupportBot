<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Customersupport">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.Customersupport.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.Customersupport.Users" />
        <activity android:name="com.Customersupport.Chat" />
        <activity
            android:name="com.Customersupport.Login"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.Customersupport.Tearms"
            android:label="@string/title_activity_tearms"
            android:theme="@style/AppTheme" />
     <activity android:name="com.Customersupport.Chatting" />
        <activity android:name="com.Customersupport.New" />
        <activity android:name="com.Customersupport.ImageListActivity" />

    </application>

</manifest>